{
    "name": "linkit-backend",
    "version": "1.0.0",
    "description": "linkit is a social media account app.",
    "main": "/dist/app.js",
    "scripts": {
        "start": "node ./dist/app.js",
        "dev": "nodemon ./src/app.ts",
        "build": "tsc -p .",
        "lint": "eslint src/**/*.ts",
        "format": "eslint src/**/*.ts --fix",
        "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
        "prepare": "husky install",
        "postinstall": "husky install",
        "prepack": "pinst --disable",
        "postpack": "pinst --enable"
    },
    "author": "https://tejaswan.netlify.app/",
    "license": "MIT",
    "dependencies": {
        "@prisma/client": "^3.15.2",
        "@types/bcrypt": "^5.0.0",
        "@types/jsonwebtoken": "^8.5.8",
        "bcrypt": "^5.0.1",
        "cloudinary": "^1.32.0",
        "cors": "^2.8.5",
        "dotenv": "^16.0.1",
        "express": "^4.18.1",
        "express-fileupload": "^1.4.0",
        "helmet": "^5.1.0",
        "joi": "^17.6.0",
        "jsonwebtoken": "^8.5.1",
        "morgan": "^1.10.0",
        "passport": "^0.6.0",
        "passport-jwt": "^4.0.0",
        "sharp": "^0.31.1",
        "swagger-ui-express": "^4.5.0"
    },
    "devDependencies": {
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/express-fileupload": "^1.4.1",
        "@types/morgan": "^1.9.3",
        "@types/node": "^18.0.0",
        "@types/passport": "^1.0.9",
        "@types/passport-jwt": "^3.0.6",
        "@types/sharp": "^0.31.0",
        "@types/swagger-ui-express": "^4.1.3",
        "@typescript-eslint/eslint-plugin": "^5.28.0",
        "@typescript-eslint/parser": "^5.28.0",
        "eslint": "^8.18.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.0.0",
        "husky": "^8.0.1",
        "nodemon": "^2.0.16",
        "pinst": "^3.0.0",
        "prettier": "^2.7.1",
        "prisma": "^3.15.2",
        "ts-node": "^10.8.1",
        "typescript": "^4.7.4"
    }
}
